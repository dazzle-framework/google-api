{
    "name": "dazzlesoftware/github",
    "type": "library",
    "description": "Dazzle Github Package",
    "keywords": ["dazzle", "framework", "github"],
    "homepage": "https://github.com/dazzle-framework/github-api",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/http": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "dazzlesoftware/uri": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Github\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Github\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
